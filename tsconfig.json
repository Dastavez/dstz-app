{
    "compileOnSave": false,
    "compilerOptions": {
        "baseUrl": "src",
        "downlevelIteration": true,
        "paths": {
            "@types": ["app/types"],
            "@libs/*": ["app/libs/*"],
            "@assets/*": ["assets/*"],
            "@styles/*": ["styles/*"],
            "@pages/*": ["app/pages/*"],
            "@helpers/*": ["app/helpers/*"],
            "@services/*": ["app/services/*"],
            "@shared/*": ["app/shared/*"],
            "@store/*": ["app/store/*"],
            "@supr/store": ["app/store"],
            "@stubs/*": ["app/stubs/*"],
            "@models": ["app/shared/models"],
            "@constants": ["app/constants"],
            "@environments/*": ["environments/*"],
            "@components/*": ["app/components/*"]
        },
        "outDir": "./dist/out-tsc",
        "sourceMap": true,
        "declaration": false,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "module": "esnext",
        "moduleResolution": "node",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "importHelpers": true,
        "target": "es5",
        "typeRoots": ["node_modules/@types"],
        "lib": ["es2018", "es2019", "dom"]
    }
}
